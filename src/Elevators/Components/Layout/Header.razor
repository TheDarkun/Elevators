<MudAppBar Color="Color.Dark" Fixed="false">
    <MudButton Href="/" Variant="Variant.Filled" Color="Color.Dark" DisableElevation>
        <MudText Typo="Typo.h6">Home</MudText>
    </MudButton>
    <MudSpacer/>
    <MudText Typo="Typo.h4">
        <b>Elevators</b>
    </MudText>
    <MudSpacer/>
    <AuthorizeView>
        <Authorized>
            <p>@AccountState.DisplayName</p>
        </Authorized>
        <NotAuthorized>
            <MudButton Href=@Configuration["uri:oauth2"] Variant="Variant.Filled" Color="Color.Dark" DisableElevation>
                <MudText Typo="Typo.h6">Sign In</MudText>
            </MudButton>
        </NotAuthorized>
    </AuthorizeView>

</MudAppBar>